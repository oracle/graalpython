ScopeEnvironment
  Scope null Module
    Symbols:
      fn: [DefLocal, Local]
    Scope fn Function
        Symbols:
            bar: [Use, GlobalImplicit]
            blat: [Use, GlobalImplicit]
      Scope lambda Function
            Flags: [IsNested, HasVarKeywords]
            Varnames: foo, blaz, z
            Symbols:
                  blaz: [DefParam, Local]
                  foo: [DefParam, Local]
                  z: [DefParam, Local]